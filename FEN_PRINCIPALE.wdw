     name : LectureSuiviPositionMédia
     internal_properties : CAAAAAgAAADs56M1B119S7SoHQJX06KZajeGv4mcc/KfRR1pfceHX3u6BfoKshQcFOnOEozEQfQiUGwcJJ4YnJmeQtYMJb80ZNmokrJW/EMJi2225lZF/rm9
     procedure_id : 1245796139411500354
     type_code : 14
     code : |1-
      // Résumé : Timer de suivi de la position dans le média pendant la lecture
      // Syntaxe :
      //LectureSuiviPositionMédia ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code
      // Elle sera répétée en boucle, en attendant 1 centièmes de seconde entre chaque appel
      // Un nouveau timer est créé pour chaque appel à la procédure
      // 
      
      procédure LectureSuiviPositionMédia()
      
      	// Lit la position actuelle de lecture
      	nPosition est un entier sans signe = MultimédiaPosition( MM_PLAYER )
      	
      	duDuréeEcoulée est une duree
      	duDuréeEcoulée.EnMillisecondes = nPosition
      		
      	// Affiche la durée écoulée et la durée restante
      	AfficheDurée( LIB_TEMPS_ECOULE, duDuréeEcoulée )
      	AfficheDurée( LIB_TEMPS_RESTANT, MM_PLAYER.DuréeTotale )
      	
      	// Mise à jour de la position	
      	POT_DUREE	= nPosition
      	JAUGE_DUREE	= nPosition
     type : 458752
   -
     name : RéorganiseAffichage
     internal_properties : CAAAAAgAAAAPoijBvx12/mYzWwS3adkULoWFxvd9cMMqvw0nNV8Qz/qp5aVjrJeZDn7MA6aSAV5Rxg9amdAqdTr0L6uGctSoUsTBYHPG2DgKTjCiWcBToIE2U7XV4Y5hSfs78jpdwE5ciVKZHfLnO4244/5S/xqif6UTrU8DiIUmc0/eoUZiGV1V+XdgCkqpHms4QuqdxfvLm/Yp8tF/WPBj34o1vWj7
     procedure_id : 1246056033321370424
     type_code : 14
     code : |1-
      // Résumé : Réorganise l'affichage
      // Syntaxe :
      // RéorganiseAffichage ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      procédure RéorganiseAffichage()
      
      	si PleinEcran ALORS
      		
      		// Cache le bouton 'Plein écran'
      		BTN_PLEIN_ECRAN.Plan			= PLAN_CACHE
      		
      		// Réaffiche le bouton de réduction
      		BTN_REDUIRE.Plan				= PLAN_PRINCIPAL
      						
      		// Cache le fond des outils de lecture
      		IMG_FOND_BARRE_LECTURE.Plan		= PLAN_CACHE
      		
      		// Démarre le timer de suivi des activités de la souris
      		TimerSensibilitéOutilsLecture()
      		
      		// Relance le timer qui cache les outils
      		LectureCacheOutils()
      		
      	sinon
      		
      		// Arrête le timer de suivi des activités de la souris
      		FinAutomatismeProcédure( TimerSensibilitéOutilsLecture )
      		FinAutomatismeProcédure( LectureCacheOutils )
      		
      		// Cache le bouton 
      		BTN_REDUIRE.Plan				= PLAN_CACHE
      		
      		// Réaffiche le bouton 'Plein écran'
      		BTN_PLEIN_ECRAN.Plan			= PLAN_PRINCIPAL
      				
      		// Réaffiche le fond des outils de lecture
      		IMG_FOND_BARRE_LECTURE.Plan		= PLAN_PRINCIPAL
      		
      		// Réaffiche les outils de lecture
      		GR_LECTURE.Visible				= Vrai
      		
      	FIN
      	
      	// Si on est en muet
      	SI Muet ALORS
      		
      		// Indique le "mute"
      		BTN_VOLUME.Plan	= FEN_PRINCIPALE.PLAN_CACHE
      		BTN_MUTE.Plan	= FEN_PRINCIPALE.PLAN_PRINCIPAL
      		
      	SINON
      		
      		// 
      		BTN_VOLUME.Plan	= PLAN_PRINCIPAL
      		BTN_MUTE.Plan	= PLAN_CACHE
      	FIN
     type : 458752
