     name : ChargeFichier
     internal_properties : CAAAAAgAAACPRk5rc30yFYTsDWfY2nudwNliJi0iZnM/tcvVE3tB+razWXaJgDeAyoZ2/+2YNSzHiy07eLmmy8gVBSLju5N8B9tb305FA5UTDMTlH6kYX10KvxkLjWJ3KwAOtUrutbMtR1LN/GAJGPiqtZiheqXDXhyyjrjBas94Bc+OfykAAw==
     procedure_id : 1245782270961887726
     type_code : 14
     code : |1-
      // Résumé : Charge le fichier spécifié
      // Syntaxe :
      //ChargeFichier (<sFichier> est chaîne)
      //
      // Paramètres :
      //	sFichier (chaîne ANSI) : Chemin complet du fichier
      // Valeur de retour :
      // 	Aucune
      //
      procédure ChargeFichier( sFichier est une chaine )
      
      	// Le fichier n'existe pas
        	si fFichierExiste( sFichier ) = faux ALORS
        		ToastAffiche( "Le fichier n'existe plus", toastCourt, cvMilieu, chCentre )
        		retour
        	FIN
        	
        	// Charge les 4 premiers octets
        	bufHeader est un buffer = fChargeBuffer( sFichier, 0, 12 )
        	bAudio est un booléen
        	
        	selon vrai
      
      		// AUDIO
        		CAS bufHeader[ 1 sur 3 ] = [ 0x49, 0x44, 0x33 ], ...																// MP3 + ID3v2
        			bufHeader[ 1 sur 2 ] = [ 0xff, 0xfb ], ...																		// MP3
        			bufHeader[ 1 sur 2 ] = [ 0xff, 0xf3 ], ...																		// MP3 + ID3v1
        			bufHeader[ 1 sur 2 ] = [ 0xff, 0xf2 ], ...																		// MP3 + ID3v1  			
        			
        			bufHeader[ 1 sur 4 ] = [ 0x52, 0x49, 0x46, 0x46 ] _ET_ bufHeader[ 9 SUR 4 ] = [ 0x57, 0x41, 0x56, 0x45 ], ...	// WAV  			
        			bufHeader[ 1 sur 4 ] = [ 0x4f, 0x67, 0x67, 0x53 ], ...															// OGG, OGA
        			bufHeader[ 1 sur 8 ] = [ 0x30, 0x26, 0xb2, 0x75, 0x8e, 0x66, 0xcf, 0x11 ], ...									// ASF, WMA
        			bufHeader[ 1 sur 8 ] = [ 0xa6, 0xd9, 0x00, 0xaa, 0x00, 0x62, 0xce, 0x6c ], ...									// ASF, WMA
        			bufHeader[ 1 sur 4 ] = [ 0x66, 0x4c, 0x61, 0x43 ]																// FLAC
        		
        			bAudio = vrai  			  					
      
      		// Les octets d'entête n'ont pas permis de savoir : on prend l'extension
        		cas minuscule( fExtraitChemin( sFichier, fExtension )[[ 2 a ]] ) dans ( "wav", "mp3", "ogg", "oga", "asf", "wma", "flac" )
        			
        			bAudio = Vrai
        		
        		// VIDEO
        		cas bufHeader[ 1 sur 4 ] = [ 0x52, 0x49, 0x46, 0x46 ] _et_ bufHeader[ 9 sur 4 ] = [ 0x41, 0x56, 0x49, 0x20 ], ...	// AVI
        			bufHeader[ 1 sur 4 ] = [ 0x1a, 0x45, 0xdf, 0xa3 ], ...															// MKV, WEBM
        			bufHeader[ 1 ] 		 = [ 0x47 ] _et_ bufHeader[ 0xbc ] = [ 0x47 ], ...											// MPG, MPEG, TS
        			bufHeader[ 1 sur 4 ] = [ 0x00, 0x00, 0x01, 0xb3 ], ...															// MPG, MPEG
        			bufHeader[ 1 sur 4 ] = [ 0x00, 0x00, 0x01, 0xba ], ...															// MPG, MPEG, VOB, M2P
        			bufHeader[ 1 sur 8 ] = [ 0x66, 0x74, 0x79, 0x70, 0x69, 0x73, 0x6f, 0x6d ], ...									// MP4
        			bufHeader[ 1 sur 3 ] = [ 0x00, 0x00, 0x00 ] _et_ bufHeader[ 5 sur 4 ] = [ 0x66, 0x74, 0x79, 0x70 ], ...			// MP4
        			bufHeader[ 5 sur 4 ] = [ 0x66, 0x74, 0x79, 0x70 ]	_et_ bufHeader[ 9 sur 4 ] = [ 0x71, 0x74, 0x20, 0x20 ]		// MOV
        			  			
        			bAudio = faux
        		
        		// Les octets d'entête n'ont pas permis de savoir : on prend l'extension
        		cas minuscule( fExtraitChemin( sFichier, fExtension )[[ 2 a ]] ) dans ( "avi", "mkv", "mpg", "mpeg", "vob", "m2p", "mp4", "webm", "mov", "ts" )
      
      			bAudio = Faux
      
      		autres cas
      			ToastAffiche( "Format de fichier non reconnu", toastCourt, cvMilieu, chCentre )  			
      			retour
        			
        	FIN
        	
        	// Arrête l'éventuelle lecture en cours
        	LectureArrête()
        			
      	// Charge le fichier
      	MultimédiaOuvre( MM_PLAYER, sFichier )
      	si ErreurDétectée ALORS
      		erreur( "Impossible d'ouvrir le fichier :", sFichier, "", erreurinfo )
      		retour
      	fin
      	
      	// Initialise la lecture
      	LectureInitialise( sFichier, bAudio ) 			
     type : 458752
