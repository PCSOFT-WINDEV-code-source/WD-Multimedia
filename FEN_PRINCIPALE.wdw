     procedures :
      -
        name : SourisValeur
        procedure_id : 1246153700909727233
        type_code : 14
        code : |1-
         // Résumé : Renvoie la valeur du potentiomètre en fonction de la position de la souris sur le potentiomètre
         // Syntaxe :
         // SourisValeur ()
         //
         // Paramètres :
         //	Aucun
         // Valeur de retour :
         // 	Aucune
         //
         procédure SourisValeur( X est un entier ) : entier
         
         	// Calcule la position en fonction de la coordonnée de la souris
         	// On décale de la moitié de la largeur du bouton du potentiomètre : c'est son centre qui détermine la position
         	// On n'utilise pas toute la largeur du potentiomètre, on enlève la largeur du bouton du potentiomètre.
         	renvoyer ( X - CoordonnéeEditeurVersEcran( 12 ) ) * POT_DUREE.BorneMax / ( POT_DUREE.Largeur - CoordonnéeEditeurVersEcran( 24 ) )
        type : 458752
      -
        name : ChangeValeur
        procedure_id : 1246154731702052596
        type_code : 14
        code : |1+
         // Résumé : Change la valeur du potentiomètre
         // Syntaxe :
         //ChangeValeur (<POS>)
         //
         // Paramètres :
         // 	POS : Nouvelle position
         // Valeur de retour :
         // 	Aucune
         //
         procédure ChangeValeur( Pos )
         
         	// Si la valeur est différente de la position actuelle
         	SI POT_DUREE <> Pos ALORS
         		
         		// Change la position actuelle
         		POT_DUREE = Pos
         		
         		// Joue le traitement de modification
         		POT_DUREE.ExécuteTraitement( trtModification )	
         	FIN
        type : 458752
