        code : |1-
         
         si MM_PLAYER.LectureEnCours = faux ALORS
         	
         	// Si la lecture était terminée
         	si MM_PLAYER.LectureTerminée ALORS
         		
         		// Redéclenche le timer de suivi de la position de la lecture
         		LectureSuiviPositionMédia()
         		
         		// Revient au début du média
         		MultimédiaPosition( MM_PLAYER, 0 )
         		MM_PLAYER.LectureTerminée = faux
         	FIN
         	
         	// Cache le bouton lecture
         	BTN_PLAY.Plan		= PLAN_CACHE
         	BTN_PAUSE.Plan		= PLAN_PRINCIPAL
         	POT_DUREE.Plan		= PLAN_PRINCIPAL
         	JAUGE_DUREE.Plan	= PLAN_PRINCIPAL
         	
         	// Si c'est une vidéo, cache le nom. Si c'est un audio, affiche le nom
         	LIB_NOM_MEDIA.Plan	= MM_PLAYER.FormatAudio ? PLAN_PRINCIPAL sinon PLAN_CACHE
         	
         	// Lecture en cours
         	MM_PLAYER.LectureEnCours = vrai		
         	IMG_ANIM_MUSIQUE.Animation = MM_PLAYER.FormatAudio
         	
         	// Joue le média
         	MultimédiaJoue( MM_PLAYER )
         	
         sinon
         	
         	// Plus de lecture en cours
         	MM_PLAYER.LectureEnCours = faux	
         	IMG_ANIM_MUSIQUE.Animation = faux
         	
         	// Met le média en pause
         	MultimédiaPause( MM_PLAYER )
         		
         	// Cache le bouton pause
         	BTN_PAUSE.Plan		= PLAN_CACHE
         	BTN_PLAY.Plan		= PLAN_PRINCIPAL
         
         	// Affiche le nom du média
         	LIB_NOM_MEDIA.Plan	= PLAN_PRINCIPAL
         FIN
        type : 18
