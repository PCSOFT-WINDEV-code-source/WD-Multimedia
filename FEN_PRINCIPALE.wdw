   -
     name : TimerSensibilitéOutilsLecture
     internal_properties : CAAAAAgAAACJ5Yf0kymR89tk5ezgoDALC/9raMfZzAZTFyXfbe77/JdRVBU9nzVA2ZW6xNcgwBqne2bdd/m74n7+KZRI7fcbO5fB/4lJ12oXzZ1NteykSRjBQBct
     procedure_id : 1245767938655763854
     type_code : 14
     code : |1-
      // Résumé : Suit l'activité de la souris pour afficher les outils de lecture en plein écran
      // Syntaxe :
      //TimerSensibilitéOutilsLecture ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code
      // Elle sera répétée en boucle, en attendant 50 centièmes de seconde entre chaque appel
      // Un nouveau timer est créé pour chaque appel à la procédure
      // 
      
      procédure TimerSensibilitéOutilsLecture()
      
      	soit CPos 	= CurseurPos( cpFenêtre )
      	soit Y 		= PoidsFort( CPos )
      	
      	// La souris survole les outils de lecture ?
      	si Y > IMG_FOND_BARRE_LECTURE.Y ALORS
      		
      		// Stoppe le timer qui cache les outils
      		FinAutomatismeProcédure( LectureCacheOutils )
      		
      		// Si les outils ne sont pas visibles
      		si GR_LECTURE.Visible = faux ALORS
      			
      			// Rend les outils visibles
      			GR_LECTURE.Visible = vrai
      	
      			// Relance le timer qui cache les outils
      			LectureCacheOutils()
      		FIN		
      	FIN
     type : 458752
   -
     name : LectureCacheOutils
     internal_properties : CAAAAAgAAAAADZh/4pXzA9Ipy+3Bb8okDW0AI5z+/M/QWy9HeiKnUS2mNblggFpgqnv43yMT0xf4z+OVBeR/RNELVw2vECNsKmKWCKpnHnE9aPAZEpwXyOsHBQgxcfEA
     procedure_id : 1245768625850560517
     type_code : 14
     code : |1-
      // Résumé : Cache les outils de lecture avec un délai
      // Syntaxe :
      //LectureCacheOutils ()
      //
      // Paramètres :
