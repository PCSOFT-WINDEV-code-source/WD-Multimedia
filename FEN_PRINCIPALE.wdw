     name : AfficheDurée
     internal_properties : CAAAAAgAAACvib71eb2fMnDLcAzh1TLpUkd/zG/43Fmn1Sb0rJGJkdcaCFwt1uVtdnXzCog2W8TNeNiYhr4j9wNSWQrb9ZHx+MzVAHwtNRelz7txuX9LN9hbWTLxFYMHdi/eiumxje++uZZDYCYS7uDyE0g=
     procedure_id : 1246129253955691972
     type_code : 14
     code : |1-
      // Résumé : Affiche la durée formattée dans le libellé spécifié
      // Syntaxe :
      //AfficheDurée (<oLibellé> est champ, <duDurée> est durée)
      //
      // Paramètres :
      //	Libellé (champ) : Libellé à utiliser
      //	duDurée (durée) : Durée à afficher
      // Valeur de retour :
      // 	Aucune
      //
      procédure AfficheDurée( Libellé est une champ, duDurée est une duree )
      
      	sDurée est une chaine
      	sDurée = duDurée.VersChaîne( "HH:MM:SS" )
      
      	// Si la durée a changé
      	si sDurée <> Libellé ALORS
      		
      		// Change la durée affichée
      		Libellé = sDurée		
      	FIN
     type : 458752
   -
     name : LectureTermine
     procedure_id : 1246129743582011336
     type_code : 14
     code : |1-
      // Résumé : Termine la lecture
      // Syntaxe :
      // LectureTermine ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      procédure LectureTermine()
      
      	// Fin de la lecture
      	MM_PLAYER.LectureEnCours	= Faux
      	MM_PLAYER.LectureTerminée	= vrai
      	
      	// Cache le bouton lecture
      	BTN_PLAY.Plan				= PLAN_PRINCIPAL
      	BTN_PAUSE.Plan				= PLAN_CACHE
      	IMG_ANIM_MUSIQUE.Animation	= faux
      	
      	FinAutomatismeProcédure( LectureSuiviPositionMédia )
      	POT_DUREE				= POT_DUREE.BorneMax
      	JAUGE_DUREE				= JAUGE_DUREE.BorneMax
      
      	// Affiche la durée restante et la durée écoulée
      	AfficheDurée( LIB_TEMPS_ECOULE, MM_PLAYER.DuréeTotale )
      	AfficheDurée( LIB_TEMPS_RESTANT, 0 )
     type : 458752
   -
     name : LectureInitialise
     procedure_id : 1246130512381271315
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //LectureInitialise (<sFichier> est chaîne, <bFormatAudio> est booléen)
      //
      // Paramètres :
      //	sFichier (chaîne ANSI) : <indiquez ici le rôle de sFichier>
      //	bFormatAudio (booléen) : <indiquez ici le rôle de bFormatAudio>
      // Valeur de retour :
      // 	Aucune
      //
      procédure LectureInitialise( sFichier est une chaine, bFormatAudio est un booléen )
      
      	// Obtient la durée totale de la vidéo
      	nDurée est un entier sans signe = MultimédiaDurée( MM_PLAYER )
      	
      	// Mémorise la durée totale
      	MM_PLAYER.DuréeTotale.EnMillisecondes	= nDurée
      	MM_PLAYER.FormatAudio					= bFormatAudio
      	
      	// Jauge de lecture
      	POT_DUREE.BorneMax						= nDurée
      	JAUGE_DUREE.BorneMax					= nDurée  			
      	POT_DUREE.Valeur						= 0
      	JAUGE_DUREE.Valeur						= 0
      	
      	LIB_LOGO.Plan				= PLAN_CACHE
      	LIB_INFO.Plan				= PLAN_CACHE
      	BTN_PLAY.Plan				= PLAN_PRINCIPAL
      	BTN_PAUSE.Plan				= PLAN_CACHE
      	POT_DUREE.Plan				= PLAN_PRINCIPAL
      	JAUGE_DUREE.Plan			= PLAN_PRINCIPAL
      	MM_PLAYER.Plan				= bFormatAudio ? PLAN_CACHE SINON PLAN_PRINCIPAL
      	IMG_ANIM_MUSIQUE.Plan		= bFormatAudio ? PLAN_PRINCIPAL SINON PLAN_CACHE
      	IMG_ANIM_MUSIQUE.Animation	= faux
      	
      	// Affiche la durée restante et la durée écoulée
      	AfficheDurée( LIB_TEMPS_ECOULE, 0 )
      	AfficheDurée( LIB_TEMPS_RESTANT, MM_PLAYER.DuréeTotale )
      	
      	// Nom de la vidéo
      	LIB_NOM_MEDIA		= fExtraitChemin( sFichier, fFichier )
      	LIB_NOM_MEDIA.Plan	= PLAN_PRINCIPAL
      	mafenetre.titre		= "WDMultimedia - " + LIB_NOM_MEDIA
      	
      	// Cadrage du nom
      	LIB_NOM_MEDIA.CadrageHorizontal = bFormatAudio ? chCentre SINON chGauche
      	LIB_NOM_MEDIA.Animation			= bFormatAudio
      	
      	// Active la lecture
      	ActiveElément( BTN_PLAY )
      	BTN_PLAY.ExécuteTraitement( trtClic )
     type : 458752
   -
     name : LectureArrête
     procedure_id : 1246184198295034616
     type_code : 14
     code : |1-
      // Résumé : Arrête une lecture en cours
      // Syntaxe :
      // LectureArrête ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      procédure LectureArrête()
      
      	// Si la lecture est en cours
      	SI MM_PLAYER.LectureEnCours ALORS
      		
      		// Arrête la lecture
      		MultimédiaArrêt( MM_PLAYER )
      		
      		// Décharge la vidéo
      		MultimédiaFerme( MM_PLAYER )
      		
      	FIN
      	
      	// Termine
      	LectureTermine()
     type : 458752
